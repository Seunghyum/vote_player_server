{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@lib/*": ["src/lib/*"],
      "@routes/*": ["src/routes/*"],
      "@models/*": ["src/models/*"],
      "@constants/*": ["src/constants/*"],
      "@scripts/*": ["src/scripts/*"]
    },
    "outDir": "dist",
    "target": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
